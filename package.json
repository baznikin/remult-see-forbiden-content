{
  "name": "demo-kitql",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:w": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "kitql-lint --format",
    "lint": "kitql-lint",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "npm run test:e2e && npm run test",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@auth/core": "^0.36.0",
    "@auth/sveltekit": "^1.6.0",
    "remult": "^0.27.0"
  },
  "devDependencies": {
    "@kitql/eslint-config": "latest",
    "@playwright/test": "^1.46.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.4.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "pg": "^8.11.3",
    "prettier": "^3.3.3",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.11",
    "tslib": "^2.7.0",
    "typescript": "5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-kit-routes": "latest",
    "vite-plugin-stripper": "latest",
    "vitest": "2.0.5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
